{
    "name": "daakibenja/ssentezo-wallet",
    "homepage": "https://github.com/daakibenja/ssentezo-wallet",
    "license": "MIT",
    "description": "A package to helpintegrate ssentezo wallet into your php applcation",
    "authors": [
        {
            "name": "Daaki Benjamin",
            "email": "richarddaaki4@gmail.com",
            "homepage": "https://github.com/hellogerard"
        }
    ],
    "require": {
        "php": ">=5.6",
        "dragonmantank/cron-expression": "^3.0",
        "opis/closure": "^3.5",
        "swiftmailer/swiftmailer": "^5.4|^6.0",
        "symfony/process": "^2.7|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "mp091689/dump-die": "^1.0",
        "phpunit/phpunit": "^4.6",
        "symfony/filesystem": "^2.7|^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "SsentezoWallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SsentezoWallet\\Tests\\": "tests/"
        }        
    },
    "config": {
        "sort-packages": true
    }
}