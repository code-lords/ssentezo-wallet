{
    "name": "daakibenja/ssentezo-wallet",
    "homepage": "https://github.com/daakibenja/ssentezo-wallet",
    "license": "MIT",
    "description": "A package to help integrate ssentezo wallet into your php application",
    "authors": [
        {
            "name": "Daaki Benjamin",
            "email": "richarddaaki4@gmail.com",
            "homepage": "https://github.com/hellogerard"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mashape/unirest-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "SsentezoWallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SsentezoWallet\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}